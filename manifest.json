{
    "manifest_version": 3,
    "name": "my miro sdk extension",
    "short_name": "mymiro",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://miro.com/app/board/*"],
            "js": ["inject.js"]
        }
    ],
    "web_accessible_resources": [{ 
        "resources": ["my_miro.js"],
        "matches": ["https://*/*"]
    }]
}
